{
    "manifest_version":2,
    "name":"My First Extension",
    "version":"1.0.1",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["content.js"]
        }
    ],
    "background":{
        "scripts":["background.js"]
    },
    "browser_action":{
        "default_icon":"cake - Copy.png"
    }
}